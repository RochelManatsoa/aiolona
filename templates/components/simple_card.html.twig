    <div class="max-w-sm mx-auto bg-gray-200 rounded-lg p-0 m-0">
      <div class="flex flex-wrap">
        <div class="mb-0 shrink-0 grow-0 basis-auto w-4/12">
          <div class="flex py-12">
            <img src="{{ imgUrl }}"
              class="z-[10] w-full rounded-lg shadow-lg ml-[50px]" alt="image" />
          </div>
        </div>

        <div class="shrink-0 grow-0 basis-auto w-8/12">
          <div class="flex h-full items-center rounded-lg bg-yellow-600 p-6 text-white pl-12 text-left">
            <div class="pl-6">
                <h2 class="mb-4 text-lg font-bold">{{ identity.firstName }} {{ identity.lastName|slice(0, 1)|upper }}.</h2>
                <p class="mb-4 text-sm pb-0">
                <span
                class="">
                {{ identity.aiCores|length }}
                </span>
                Outils AI<br>
                <span class="text-xs flex">{{ identity.tarif }} € / heure</span><br>
                  {% for lang in identity.languages %}
                  {{ isoToEmoji(lang.code) }}
                  {% endfor %}
                <span class="text-xs flex">
                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-4 h-4">
                    <path stroke-linecap="round" stroke-linejoin="round" d="M16.5 18.75h-9m9 0a3 3 0 013 3h-15a3 3 0 013-3m9 0v-3.375c0-.621-.503-1.125-1.125-1.125h-.871M7.5 18.75v-3.375c0-.621.504-1.125 1.125-1.125h.872m5.007 0H9.497m5.007 0a7.454 7.454 0 01-.982-3.172M9.497 14.25a7.454 7.454 0 00.981-3.172M5.25 4.236c-.982.143-1.954.317-2.916.52A6.003 6.003 0 007.73 9.728M5.25 4.236V4.5c0 2.108.966 3.99 2.48 5.228M5.25 4.236V2.721C7.456 2.41 9.71 2.25 12 2.25c2.291 0 4.545.16 6.75.47v1.516M7.73 9.728a6.726 6.726 0 002.748 1.35m8.272-6.842V4.5c0 2.108-.966 3.99-2.48 5.228m2.48-5.492a46.32 46.32 0 012.916.52 6.003 6.003 0 01-5.395 4.972m0 0a6.726 6.726 0 01-2.749 1.35m0 0a6.772 6.772 0 01-3.044 0" />
                </svg>{{ identity.experiences|length }} projets réalisés</span>
                </p>
            </div>
          </div>
        </div>
      </div>
      <div class="p-8">
      {% for star in stars %}
          <p class="bg-gray-200 rounded-full px-3 my-2 text-xs font-base text-gray-700 mr-2">{{ star.name }}</p>
          <div class="flex items-center">
              {% for i in 1..5 %}
                  {% if i <= star.note %}
                      <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-yellow-500" viewBox="0 0 20 20" fill="currentColor">
                          <path d="M10 1L12.39 6.75H19.78L14.76 10.25L17.17 16L10 12.75L2.83 16L5.24 10.25L0.22 6.75H7.61L10 1Z" />
                      </svg>
                  {% else %}
                      <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-gray-300" viewBox="0 0 20 20" fill="currentColor">
                          <path d="M10 1L12.39 6.75H19.78L14.76 10.25L17.17 16L10 12.75L2.83 16L5.24 10.25L0.22 6.75H7.61L10 1Z" />
                      </svg>
                  {% endif %}
              {% endfor %}
          </div>
      {% endfor %}
      
        <a
        href="{{ url }}"
        class="inline-block rounded bg-warning m-2 px-6 pb-2 pt-2.5 text-xs font-medium uppercase leading-normal text-white shadow-[0_4px_9px_-4px_#e4a11b] transition duration-150 ease-in-out hover:bg-warning-600 hover:shadow-[0_8px_9px_-4px_rgba(228,161,27,0.3),0_4px_18px_0_rgba(228,161,27,0.2)] focus:bg-warning-600 focus:shadow-[0_8px_9px_-4px_rgba(228,161,27,0.3),0_4px_18px_0_rgba(228,161,27,0.2)] focus:outline-none focus:ring-0 active:bg-warning-700 active:shadow-[0_8px_9px_-4px_rgba(228,161,27,0.3),0_4px_18px_0_rgba(228,161,27,0.2)] dark:shadow-[0_4px_9px_-4px_rgba(228,161,27,0.5)] dark:hover:shadow-[0_8px_9px_-4px_rgba(228,161,27,0.2),0_4px_18px_0_rgba(228,161,27,0.1)] dark:focus:shadow-[0_8px_9px_-4px_rgba(228,161,27,0.2),0_4px_18px_0_rgba(228,161,27,0.1)] dark:active:shadow-[0_8px_9px_-4px_rgba(228,161,27,0.2),0_4px_18px_0_rgba(228,161,27,0.1)]">
        View profile
        </a>
        <button
        type="button"
        class="inline-block rounded border-2 border-primary-100 m-2 px-6 pb-[6px] pt-2 text-xs font-medium uppercase leading-normal text-yellow-600 transition duration-150 ease-in-out hover:border-primary-accent-100 hover:bg-neutral-500 hover:bg-opacity-10 focus:border-primary-accent-100 focus:outline-none focus:ring-0 active:border-primary-accent-200 dark:text-primary-100 dark:hover:bg-neutral-100 dark:hover:bg-opacity-10"
        data-te-ripple-init>
        Add to favorite
        </button>
      </div>
    </div>
