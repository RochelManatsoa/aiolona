                    <div class="w-full md:w-1/3 px-2">
                        <div class="p-4">
                            Adresse email
                        </div>
                        <div class="p-4">
                            Téléphone
                        </div>
                    </div>

                    <!-- Deuxième colonne -->
                    <div class="w-full md:w-1/3 px-2">
                        <div class="p-4 font-semibold">
                            {{ identity.user.email }}
                        </div>
                        <div class="p-4 font-semibold">
